.edit-form-container {
  height: 100%;
  width: 510px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.edit-profile-avatar-wrapper {
  width: 130px;
  height: 130px;
  margin-bottom: 60px;
}

.submit-button {
  width: 280px;
  height: 37px;
  margin: 80px 0 18px;
}

.cancel-button {
  color: var(--color-primary);
  background-color: transparent;
}
