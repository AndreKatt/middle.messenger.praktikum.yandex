.error-wrapper {
  height: 100vh;
  display: flex;
  justify-content: center;
}

.error-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.error-title {
  font-size: 40px;
  font-weight: 500;
}

.error-description {
  font-size: 20px;
  margin-top: 20px;
  font-weight: 500;
}

.error-link {
  margin-top: 80px;
}
